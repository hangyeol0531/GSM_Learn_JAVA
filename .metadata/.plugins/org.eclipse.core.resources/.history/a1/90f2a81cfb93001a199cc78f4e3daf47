import gsm.poly.A;
import gsm.poly.Animal;
import gsm.poly.B;
public class GSM23 {

	public static void main(String[] args) {
		
		
		Object[] obj=new Object[2]; 
		obj[0] = new A();
		obj[1] = new B();

		A a = new A();
		display(a);
		
		B b = new B();
		display(b);
	}
	
	public static void display(Object obj) { //중요 
		if(obj instanceof A) {
			((A)obj).ptr();
		}else if(obj instanceof B){
			((B)obj).ptr();
		}
	}

}
