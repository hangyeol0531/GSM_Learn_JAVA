		// A클래스의 객체를 upcasting으로 생성해서 ptr()으로 동작
		
		Object obj = new A();
		((A)obj).ptr(); //X --> 수정 
		
		Object obj = new B();
		((B)obj).ptr(); //X --> 수정 
	}

